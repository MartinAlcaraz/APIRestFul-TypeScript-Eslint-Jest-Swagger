  "scripts": {
    "build": "npm run clean && npx tsc && npm run copy-files",
    "build:dev": "npx webpack --mode development",
    "build:prod": "npx webpack --mode production",
    "dev": "concurrently \"npm run swagger\" \"npx tsc --watch\" \"nodemon -q ./dist/index.js\" \"npm run copy-files\" ",
    "dev2": "concurrently \"nodemon -x tsoa spec\" \"npx tsc --watch\" \"nodemon -q ./dist/index.js\" \"npm run copy-files\" ",
    "test": "jest",
    "serve:coverage": "npm run test && cd coverage/lcov-report && npx serve",
    "start": "node ./dist/index.js",
    "clean": "rimraf ./dist",
    "copy-files": "copyfiles -u 1 ./public/*.json ./dist/public",
    "swagger": "tsoa spec"
  },
